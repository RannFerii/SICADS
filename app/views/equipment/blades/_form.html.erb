<%= simple_form_for(@equipment_blade) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nomenclatura %>
    <%= f.input :cuchilla_type %>
    <%= f.input :mecanismo_type %>
    <%= f.input :nivel_basico_impulso %>
    <%= f.input :marca %>
    <%= f.input :num_serie %>
    <%= f.input :kv_nom %>
    <%= f.input :amp_nom %>
    <%= f.input :tension_sistema %>
    <%= f.input :fecha_puesta_servicio %>
    <%= f.input :fecha_fabricacion %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
