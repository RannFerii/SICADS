<%= simple_form_for(['equipments',@battery_bank]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nomenclatura %>
    <%= f.input :marca %>
    <%= f.input :banco_baterias_type %>
    <%= f.input :num_celdas %>
    <%= f.input :capacidad_ah %>
    <%= f.input :volts_totales %>
    <%= f.input :fecha_puesta_servicio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
