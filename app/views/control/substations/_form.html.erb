<%= simple_form_for(['control',@substation]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre_subestacion %>
    <%= f.input :subestacion_abbr %>
    <%= f.input :direccion %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :extencion %>
    <%= f.input :encargado_nombre %>
    <%= f.input :encargado_apellidos %>
    <%= f.input :capacidad %>
    <%= f.input :fecha_puesta_servicio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
